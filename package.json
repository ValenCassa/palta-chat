  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:db": "cd server/supabase && source .env && supabase start",
    "db-prod:push": "source .env.prod && drizzle-kit push",
    "db:push": "drizzle-kit push",
